Order Allow,Deny
Allow from all
Deny from none

ErrorDocument 404 http://hcwk.viwid.com/404

RewriteEngine On

#RewriteCond %{HTTP_HOST} !^hcwk.viwid.com$ [NC]
#RewriteRule ^(.*) http://hcwk.viwid.com/$1 [R=301,L]

RewriteCond %{REQUEST_FILENAME} -f
#RewriteCond %{REQUEST_FILENAME} -d
RewriteRule  .?  -  [S=2,L]

RewriteRule ^referent/([a-z0-9-]+)/?$ referenten/#$1 [QSA,NC,L]
RewriteRule ^vortrag/([a-z0-9-]+)/?$ index.php?p=vortrag&id=$1 [QSA,NC,L]
RewriteRule ^news/([a-z0-9-]+)/?$ index.php?p=news-item&id=$1 [QSA,NC,L]
RewriteRule ^([a-z0-9-]+)/?$ index.php?p=$1 [QSA,NC,L]
RewriteRule ^(.*) index.php [QSA,L]
